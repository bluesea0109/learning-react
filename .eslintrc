{
  "extends": "eslint-config-airbnb",
  "parser": "babel-eslint",
  "rules": {
    "react/prefer-es6-class": [
      0
    ],
    "no-var": [
      0
    ],
    "no-console": [
      0
    ],
    "func-names": [
      0
    ],
    "space-before-function-paren": [
      0
    ],
    "no-param-reassign": [
      0
    ],
    "vars-on-top": [
      0
    ],
    "react/no-multi-comp": [0]
  },
  "env": {
    "node": true,
    "mocha": true
  }
}